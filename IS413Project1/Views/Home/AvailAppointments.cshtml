@{
}
@model AppointmentsListViewModel
<!--SignUpPage: This will display the available times. Once they select a time they are routed to a form that has the date info set-->
<!--This is an example of you can display all of the seed data-->

@foreach (var x in Model.Booked)
{
    <div class="card card-outline-primary m-1 p-1">
        <div class=“bg-faded p-1">
            <h3>@x.BeginTime</h3>

            <p>@x.BeginTime</p>
            <p>@x.Duration</p>
            <p>@x.Description</p>
            <span class=“badge badge-pill badge-primary” style=“float:right”>
                <small>@x.Description</small>
            </span>
        </div>
    </div>
}
